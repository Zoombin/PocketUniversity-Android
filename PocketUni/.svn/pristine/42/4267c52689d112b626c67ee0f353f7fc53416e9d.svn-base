package com.xyhui.types;

import android.os.Parcel;
import android.os.Parcelable;

import com.mslibs.utils.ParcelUtils;
import com.xyhui.utils.SpanUtils;

public class GroupDoc implements Parcelable {

	public String id = null;
	public String uid = null;
	public String name = null;
	public String note = null;
	public String filesize = null;
	public String filetype = null;
	public String fileurl = null;
	public String totaldowns = null;
	public String ctime = null;
	public String attachId = null;

	public String getDesc() {

		long filelength = 0;

		try {
			filelength = Long.parseLong(filesize);
		} catch (NumberFormatException e) {
			e.printStackTrace();
		}
		return "大小：" + SpanUtils.FormetFileSize(filelength) + " 下载：" + totaldowns + "次 \n上传时间："
				+ SpanUtils.getTimeString(ctime);
	}

	@Override
	public int describeContents() {
		return 0;
	}

	@Override
	public void writeToParcel(Parcel out, int flags) {

		ParcelUtils.writeStringToParcel(out, id);
		ParcelUtils.writeStringToParcel(out, uid);
		ParcelUtils.writeStringToParcel(out, name);
		ParcelUtils.writeStringToParcel(out, note);
		ParcelUtils.writeStringToParcel(out, filesize);
		ParcelUtils.writeStringToParcel(out, filetype);
		ParcelUtils.writeStringToParcel(out, fileurl);
		ParcelUtils.writeStringToParcel(out, totaldowns);
		ParcelUtils.writeStringToParcel(out, ctime);
		ParcelUtils.writeStringToParcel(out, attachId);

	}

	public GroupDoc() {
	}

	public GroupDoc(Parcel in) {

		id = ParcelUtils.readStringFromParcel(in);
		uid = ParcelUtils.readStringFromParcel(in);
		name = ParcelUtils.readStringFromParcel(in);
		note = ParcelUtils.readStringFromParcel(in);
		filesize = ParcelUtils.readStringFromParcel(in);
		filetype = ParcelUtils.readStringFromParcel(in);
		fileurl = ParcelUtils.readStringFromParcel(in);
		totaldowns = ParcelUtils.readStringFromParcel(in);
		ctime = ParcelUtils.readStringFromParcel(in);
		attachId = ParcelUtils.readStringFromParcel(in);
	}

	public static final Parcelable.Creator<GroupDoc> CREATOR = new Parcelable.Creator<GroupDoc>() {
		public GroupDoc createFromParcel(Parcel in) {
			return new GroupDoc(in);
		}

		@Override
		public GroupDoc[] newArray(int size) {
			return new GroupDoc[size];
		}
	};
}
