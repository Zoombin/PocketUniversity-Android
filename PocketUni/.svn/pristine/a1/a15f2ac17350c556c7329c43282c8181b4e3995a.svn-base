package com.xyhui.types;

import android.os.Parcel;
import android.os.Parcelable;

import com.google.gson.annotations.SerializedName;
import com.mslibs.utils.ParcelUtils;

public class WeiboTypeData implements Parcelable {
	public String thumburl = null;
	public String thumbmiddleurl = null;
	public String picurl = null;
	public int attach_id = 0;

	@SerializedName("0")
	public WeiboTypeData data = null;

	public void fix() {
		if (data != null) {
			picurl = picurl + data.picurl;
			thumburl = thumburl + data.thumburl;
			thumbmiddleurl = thumbmiddleurl + data.thumbmiddleurl;
			data = null;
		}
	}

	@Override
	public int describeContents() {
		return 0;
	}

	@Override
	public void writeToParcel(Parcel out, int flags) {
		ParcelUtils.writeStringToParcel(out, thumburl);
		ParcelUtils.writeStringToParcel(out, thumbmiddleurl);
		ParcelUtils.writeStringToParcel(out, picurl);
	}

	public WeiboTypeData() {
	}

	public WeiboTypeData(Parcel in) {

		thumburl = ParcelUtils.readStringFromParcel(in);
		thumbmiddleurl = ParcelUtils.readStringFromParcel(in);
		picurl = ParcelUtils.readStringFromParcel(in);

	}

	public static final Parcelable.Creator<WeiboTypeData> CREATOR = new Parcelable.Creator<WeiboTypeData>() {
		public WeiboTypeData createFromParcel(Parcel in) {
			return new WeiboTypeData(in);
		}

		@Override
		public WeiboTypeData[] newArray(int size) {
			return new WeiboTypeData[size];
		}
	};
}
