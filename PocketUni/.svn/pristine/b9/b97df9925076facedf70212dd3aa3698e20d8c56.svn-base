<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/navbar"
        android:orientation="vertical"
        android:padding="6dip" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone" >

            <RelativeLayout
                android:id="@+id/layout_app_xntz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_xntz"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_xntz" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_xntz"
                    android:gravity="center"
                    android:text="通知"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_yxhd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_xyhd"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_xyhd" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_xyhd"
                    android:gravity="center"
                    android:text="活动"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_st"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_st"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_st" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_st"
                    android:gravity="center"
                    android:text="部落"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_course"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_course"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_course" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_course"
                    android:gravity="center"
                    android:text="课程"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <RelativeLayout
                android:id="@+id/layout_app_gamecenter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_gamecenter"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_gamecenter" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_gamecenter"
                    android:gravity="center"
                    android:text="游戏中心"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_shake"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_shake"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_yyy" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_shake"
                    android:gravity="center"
                    android:text="摇一摇"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_train"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_train"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_train" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_train"
                    android:gravity="center"
                    android:text="培训"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_jqt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_jqt"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_jqt" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_jqt"
                    android:gravity="center"
                    android:text="街区淘"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <RelativeLayout
                android:id="@+id/layout_app_qnh"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_qnh"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_qnh" />

                <TextView
                    android:id="@+id/text_qnh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_qnh"
                    android:ellipsize="marquee"
                    android:singleLine="true"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_139yx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_volunteer"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_volunteer" />

                <TextView
                    android:id="@+id/txt_app_volunteer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_volunteer"
                    android:ellipsize="marquee"
                    android:singleLine="true"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_139yx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_volunteer"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_volunteer" />

                <TextView
                    android:id="@+id/txt_app_volunteer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_volunteer"
                    android:ellipsize="marquee"
                    android:singleLine="true"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_wishhelp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_wishhelp"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_wish" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_wishhelp"
                    android:gravity="center"
                    android:text="愿与助"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <RelativeLayout
                android:id="@+id/layout_app_zp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_ly"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_ly" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_ly"
                    android:gravity="center"
                    android:text="旅游"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_mobilezone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_mobilezone"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_operator" />

                <TextView
                    android:id="@+id/text_operator_zone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_mobilezone"
                    android:ellipsize="marquee"
                    android:gravity="center"
                    android:singleLine="true"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_charge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1" >

                <ImageButton
                    android:id="@+id/btn_app_charge"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_charge" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_charge"
                    android:gravity="center"
                    android:text="充值"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_app_xf1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="6dip"
                android:layout_weight="1">

                <ImageButton
                    android:id="@+id/btn_app_miaopai"
                    android:layout_width="66dip"
                    android:layout_height="66dip"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/btn_selector_app"
                    android:padding="3dip"
                    android:scaleType="fitXY"
                    android:src="@drawable/app_icon_miaopai" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_app_miaopai"
                    android:gravity="center"
                    android:text="秒拍"
                    android:textColor="#333"
                    android:textSize="14sp" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/navbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/navbar_TitleText"
            style="@style/navigation_title"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_alignParentTop="true"
            android:background="@drawable/bar_navigation_bg"
            android:gravity="center"
            android:text="应用" />

        <com.xyhui.widget.AdBannerLayout
            android:id="@+id/ad_banner"
            android:layout_width="match_parent"
            android:layout_height="65dip"
            android:layout_below="@+id/navbar_TitleText"
            android:layout_marginTop="-6dip" />
    </RelativeLayout>

</RelativeLayout>