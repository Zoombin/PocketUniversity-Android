package com.xyhui.types;

import android.os.Parcel;
import android.os.Parcelable;

import com.google.gson.JsonElement;
import com.mslibs.utils.ParcelUtils;

public class Weibo implements Parcelable {
	public String weibo_id = null;
	public String uid = null;
	public String content = null;
	public String ctime = null;
	public String from = null;
	public String comment = null;
	public String transpond_id = null;
	public String transpond = null;
	public String type = null;
	public WeiboTypeData type_data = null;
	public JsonElement from_data = null;
	public int favorited = 0;
	public String isdel = null;
	public String uname = null;
	public String face = null;
	public JsonElement transpond_data = null;
	public String timestamp = null;

	@Override
	public int describeContents() {
		return 0;
	}

	@Override
	public void writeToParcel(Parcel out, int flags) {
		ParcelUtils.writeStringToParcel(out, weibo_id);
		ParcelUtils.writeStringToParcel(out, uid);
		ParcelUtils.writeStringToParcel(out, content);
		ParcelUtils.writeStringToParcel(out, ctime);
		ParcelUtils.writeStringToParcel(out, from);
		ParcelUtils.writeStringToParcel(out, comment);
		ParcelUtils.writeStringToParcel(out, transpond_id);
		ParcelUtils.writeStringToParcel(out, transpond);
		ParcelUtils.writeStringToParcel(out, type);
		ParcelUtils.writeStringToParcel(out, favorited + "");
		ParcelUtils.writeStringToParcel(out, isdel);
		ParcelUtils.writeStringToParcel(out, uname);
		ParcelUtils.writeStringToParcel(out, face);
		ParcelUtils.writeStringToParcel(out, timestamp);
	}

	public Weibo() {

	}

	public Weibo(Parcel in) {
		weibo_id = ParcelUtils.readStringFromParcel(in);
		uid = ParcelUtils.readStringFromParcel(in);
		content = ParcelUtils.readStringFromParcel(in);
		ctime = ParcelUtils.readStringFromParcel(in);
		from = ParcelUtils.readStringFromParcel(in);
		comment = ParcelUtils.readStringFromParcel(in);
		transpond_id = ParcelUtils.readStringFromParcel(in);
		transpond = ParcelUtils.readStringFromParcel(in);
		type = ParcelUtils.readStringFromParcel(in);
		favorited = Integer.parseInt(ParcelUtils.readStringFromParcel(in));
		isdel = ParcelUtils.readStringFromParcel(in);
		uname = ParcelUtils.readStringFromParcel(in);
		face = ParcelUtils.readStringFromParcel(in);
		timestamp = ParcelUtils.readStringFromParcel(in);
	}

	public static final Parcelable.Creator<Weibo> CREATOR = new Parcelable.Creator<Weibo>() {
		public Weibo createFromParcel(Parcel in) {
			return new Weibo(in);
		}

		@Override
		public Weibo[] newArray(int size) {
			return new Weibo[size];
		}
	};
}
