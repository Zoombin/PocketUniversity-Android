package com.xyhui.types;

import java.util.ArrayList;

public class Topic {
	public String tid = null;
	public int start_floor = 0;
	public Thread topic = null;
	public ArrayList<Reply> reply = null;

	public class Thread {
		public String id = null;
		public String gid = null;
		public String uid = null;
		public String name = null;
		public String title = null;
		public String cid = null;
		public String viewcount = null;
		public String replycount = null;
		public String addtime = null;
		public String replytime = null;
		public String content = null;
		public String pid = null;
		public String face = null;
	}

	public class Reply {
		public String id = null;
		public String gid = null;
		public String uid = null;
		public String tid = null;
		public String content = null;
		public String ctime = null;
		public String ip = null;
