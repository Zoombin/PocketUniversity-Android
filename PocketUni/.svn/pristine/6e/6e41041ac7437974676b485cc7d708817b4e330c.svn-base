package com.xyhui.types;

import android.os.Parcel;
import android.os.Parcelable;

import com.mslibs.utils.ParcelUtils;

public class Comment implements Parcelable {
	public String comment_id = null;
	public String uid = null;
	public String content = null;
	public String ctime = null;
	public String uname = null;
	public int timestamp = 0;

	@Override
	public int describeContents() {
		return 0;
	}

	@Override
	public void writeToParcel(Parcel out, int flags) {
		ParcelUtils.writeStringToParcel(out, comment_id);
		ParcelUtils.writeStringToParcel(out, uid);
		ParcelUtils.writeStringToParcel(out, content);
		ParcelUtils.writeStringToParcel(out, ctime);
		ParcelUtils.writeStringToParcel(out, uname);
	}

	public Comment() {
	}

	public Comment(Parcel in) {

		comment_id = ParcelUtils.readStringFromParcel(in);
		uid = ParcelUtils.readStringFromParcel(in);
		content = ParcelUtils.readStringFromParcel(in);
		ctime = ParcelUtils.readStringFromParcel(in);
		uname = ParcelUtils.readStringFromParcel(in);
	}

	public static final Parcelable.Creator<Comment> CREATOR = new Parcelable.Creator<Comment>() {
		public Comment createFromParcel(Parcel in) {
			return new Comment(in);
		}

		@Override
		public Comment[] newArray(int size) {
			return new Comment[size];
		}
	};

}
